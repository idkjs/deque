    "bs-platform": "^9.0.2",
    "esy": "^0.6.10"
  },
  "jest": {
    "testURL": "http://localhost/",
    "testRegex": "Test.bs.js",
    "transformIgnorePatterns": [
      "/node_modules/(?!@glennsl/bs-jest|bs-platform).+\\.js$"
